{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "Eclipse"
    ],
    "contact": {
        "issues": "https://github.com/Deftu/${mod_name}/issues",
        "sources": "https://github.com/Deftu/${mod_name}"
    },
    "license": "LGPL-3.0-or-later",
    "environment": "*",
    "accessWidener": "stella.accesswidener",
    "entrypoints": {
        "client": [
            {
                "value": "co.stellarskys.stella.Stella",
                "adapter": "kotlin"
            }
        ]
    },
    "mixins": [
        "mixins.stella.json"
    ],
    "depends": {
        "minecraft": "~${minor_mc_version}",
        "fabric": "*",
        "fabricloader": ">=0.15.11",
        "fabric-language-kotlin": "*"
    }
}
